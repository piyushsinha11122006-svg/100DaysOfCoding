// 111. First Negative in Each Window of Size K
#include <stdio.h>

int main() {
    int n, k;
    scanf("%d%d",&n,&k);

    int a[n];
    for(int i=0;i<n;i++) scanf("%d",&a[i]);

    for(int i=0;i<=n-k;i++) {
        int found = 0;
        for(int j=i;j<i+k;j++) {
            if(a[j] < 0) {
                printf("%d ", a[j]);
                found = 1;
                break;
            }
        }
        if(!found) printf("0 ");
    }

    return 0;
}

// Q112 — Maximum Subarray Sum (Kadane's Algorithm)
#include <stdio.h>

int main() {
    int n;
    scanf("%d",&n);
    int a[n];

    for(int i=0;i<n;i++) scanf("%d",&a[i]);

    int maxSum = a[0], curr = 0;

    for(int i=0;i<n;i++) {
        curr += a[i];
        if(curr > maxSum) maxSum = curr;
        if(curr < 0) curr = 0;
    }

    printf("%d", maxSum);
    return 0;
}
// Q113 — Kth Smallest Element (Sort)
#include <stdio.h>

void sort(int a[], int n) {
    for(int i=0;i<n;i++)
        for(int j=i+1;j<n;j++)
            if(a[j] < a[i]) {
                int t=a[i]; a[i]=a[j]; a[j]=t;
            }
}

int main() {
    int n, k;
    scanf("%d%d",&n,&k);

    int a[n];
    for(int i=0;i<n;i++) scanf("%d",&a[i]);

    sort(a,n);

    printf("%d", a[k-1]);
    return 0;
}

// Q114 — Longest Substring Without Repeating Characters
#include <stdio.h>
#include <string.h>

int main() {
    char s[200];
    scanf("%s", s);

    int freq[256] = {0}, l=0, ans=0;

    for(int r=0; s[r]!='\0'; r++) {
        freq[(int)s[r]]++;

        while(freq[(int)s[r]] > 1) {
            freq[(int)s[l]]--;
            l++;
        }

        int len = r - l + 1;
        if(len > ans) ans = len;
    }

    printf("%d", ans);
    return 0;
}

// Q115 — Valid Anagram
#include <stdio.h>
#include <string.h>

int main() {
    char s[200], t[200];
    scanf("%s %s", s, t);

    if(strlen(s) != strlen(t)) {
        printf("Not Anagram");
        return 0;
    }

    int freq[26] = {0};

    for(int i=0;s[i];i++) freq[s[i]-'a']++;
    for(int i=0;t[i];i++) freq[t[i]-'a']--;

    for(int i=0;i<26;i++)
        if(freq[i] != 0) {
            printf("Not Anagram");
            return 0;
        }

    printf("Anagram");
    return 0;
}

// Q116 — Two Sum
#include <stdio.h>

int main() {
    int n, target;
    scanf("%d",&n);

    int a[n];
    for(int i=0;i<n;i++) scanf("%d",&a[i]);

    scanf("%d",&target);

    for(int i=0;i<n;i++) {
        for(int j=i+1;j<n;j++) {
            if(a[i] + a[j] == target) {
                printf("%d %d", i, j);
                return 0;
            }
        }
    }

    printf("-1 -1");
    return 0;
}

// Q117 – Merge Two Sorted Arrays
#include <stdio.h>

int main(){
    int m,n;
    scanf("%d %d",&m,&n);
    int a[m], b[n];

    for(int i=0;i<m;i++) scanf("%d",&a[i]);
    for(int i=0;i<n;i++) scanf("%d",&b[i]);

    int i=0,j=0;
    while(i<m && j<n){
        if(a[i] < b[j]) printf("%d ", a[i++]);
        else printf("%d ", b[j++]);
    }
    while(i<m) printf("%d ", a[i++]);
    while(j<n) printf("%d ", b[j++]);
}

// Q118 – Missing Number (0 to n)
#include <stdio.h>

int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    int sum=0;

    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
        sum += arr[i];
    }

    int total = n*(n+1)/2;
    printf("%d", total - sum);
}

// Q119 – Find Repeated Element (Single Pass)
#include <stdio.h>

int main(){
    int n;
    scanf("%d",&n);
    int arr[n], freq[100000]={0};

    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
        if(freq[arr[i]]==1){
            printf("%d", arr[i]);
            return 0;
        }
        freq[arr[i]] = 1;
    }
}

// 120. Convert String to Sentence Case
#include <stdio.h>
#include <ctype.h>

int main() {
    char str[200];
    
    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    // Convert full string to lowercase
    for(int i = 0; str[i] != '\0'; i++) {
        str[i] = tolower(str[i]);
    }

    // Convert first non-space character to uppercase
    int i = 0;
    while(str[i] == ' ' && str[i] != '\0') {
        i++;
    }
    if(str[i] != '\0') {
        str[i] = toupper(str[i]);
    }

    printf("Sentence case: %s", str);

    return 0;
}
