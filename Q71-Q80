// 71. print matrix.
#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);

    int a[r][c];

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<r; i++) {
        for(int j=0; j<c; j++)
            printf("%d ", a[i][j]);
        printf("\n");
    }
    return 0;
}

// 72. sum of all elements in a matrix.
#include <stdio.h>

int main() {
    int r, c, sum = 0;
    scanf("%d %d", &r, &c);

    int a[r][c];

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++) {
            scanf("%d", &a[i][j]);
            sum += a[i][j];
        }

    printf("Sum = %d", sum);
    return 0;
}

// 73. sum of each row of a matrix and store it in an array.
#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);

    int a[r][c], rowSum[r];

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<r; i++) {
        rowSum[i] = 0;
        for(int j=0; j<c; j++)
            rowSum[i] += a[i][j];
    }

    for(int i=0; i<r; i++)
        printf("%d ", rowSum[i]);

    return 0;
}

// 74. transposa of a matrix.
#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);

    int a[r][c], t[c][r];

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            t[j][i] = a[i][j];

    for(int i=0; i<c; i++) {
        for(int j=0; j<r; j++)
            printf("%d ", t[i][j]);
        printf("\n");
    }
    return 0;
}

// 75. add two matriz.
#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);

    int a[r][c], b[r][c], s[r][c];

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &b[i][j]);

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            s[i][j] = a[i][j] + b[i][j];

    for(int i=0; i<r; i++) {
        for(int j=0; j<c; j++)
            printf("%d ", s[i][j]);
        printf("\n");
    }
    return 0;
}

// 76. check symmetric matrix.
#include <stdio.h>

int main() {
    int n, flag=1;
    scanf("%d", &n);

    int a[n][n];
    for(int i=0; i<n; i++)
        for(int j=0; j<n; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<n; i++)
        for(int j=0; j<n; j++)
            if(a[i][j] != a[j][i]) flag = 0;

    if(flag) printf("Symmetric");
    else printf("Not symmetric");

    return 0;
}

// 77. check distinct diagonal element.
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int a[n][n], used[n], flag=1;

    for(int i=0; i<n; i++) used[i] = 0;

    for(int i=0; i<n; i++)
        for(int j=0; j<n; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<n; i++) {
        for(int j=i+1; j<n; j++)
            if(a[i][i] == a[j][j])
                flag = 0;
    }

    if(flag) printf("Distinct");
    else printf("Not distinct");

    return 0;
}

// 78. sum of main diagonal elements for a square matrix.
#include <stdio.h>

int main() {
    int n, sum = 0;
    scanf("%d", &n);

    int a[n][n];
    for(int i=0; i<n; i++)
        for(int j=0; j<n; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<n; i++)
        sum += a[i][i];

    printf("Sum = %d", sum);
    return 0;
}

// 79. perform diagonal traversal of a matrix.
#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);

    int a[r][c];
    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &a[i][j]);

    // Upper diagonals including main
    for(int start=0; start<c; start++) {
        int i=0, j=start;
        while(i<r && j>=0) {
            printf("%d ", a[i][j]);
            i++; j--;
        }
        printf("\n");
    }

    // Lower diagonals
    for(int start=1; start<r; start++) {
        int i=start, j=c-1;
        while(i<r && j>=0) {
            printf("%d ", a[i][j]);
            i++; j--;
        }
        printf("\n");
    }

    return 0;
}

// 80. multiply two matrices.
#include <stdio.h>

int main() {
    int r1,c1,r2,c2;
    scanf("%d %d", &r1, &c1);
    scanf("%d %d", &r2, &c2);

    if(c1 != r2) {
        printf("Multiplication not possible");
        return 0;
    }

    int a[r1][c1], b[r2][c2], m[r1][c2];

    for(int i=0; i<r1; i++)
        for(int j=0; j<c1; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<r2; i++)
        for(int j=0; j<c2; j++)
            scanf("%d", &b[i][j]);

    for(int i=0; i<r1; i++)
        for(int j=0; j<c2; j++) {
            m[i][j] = 0;
            for(int k=0; k<c1; k++)
                m[i][j] += a[i][k] * b[k][j];
        }

    for(int i=0; i<r1; i++) {
        for(int j=0; j<c2; j++)
            printf("%d ", m[i][j]);
        printf("\n");
    }
    return 0;
}
